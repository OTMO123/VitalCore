# VitalCore Frontend Docker Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws

# Development Settings
VITE_NODE_ENV=development
VITE_DEBUG=true

# Healthcare Platform Settings
VITE_PLATFORM_NAME=VitalCore
VITE_VERSION=1.0.0

# Security Settings
VITE_ENABLE_MOCK_AUTH=true
VITE_MOCK_USER_ROLE=doctor

# Feature Flags
VITE_ENABLE_VOICE_RECOGNITION=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_SPECIALIST_MODE=true

# Integration Settings
VITE_ENABLE_FHIR_VALIDATION=true
VITE_ENABLE_AUDIT_LOGGING=true
VITE_ENABLE_PHI_ENCRYPTION=true